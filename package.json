{
  "name": "supabase-edge-function-frontend",
  "private": true,
  "version": "1.0.0",
  "author": "Robert Anthony Tribiana",
  "description": "Supabase Edge Function Frontend",
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check build-only",
    "build-only": "vite build",
    "build-test": "npm run build && npm run test",
    "test:unit": "jest --config jest.config.js --forceExit --detectOpenHandles",
    "type-check": "vue-tsc --noEmit --composite false",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/",
    "supabase": "supabase"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.44.2",
    "axios": "^1.7.2",
    "vue": "^3.4.29"
  },
  "devDependencies": {
    "@playwright/test": "^1.45.0",
    "@rushstack/eslint-patch": "^1.10.3",
    "@tsconfig/node18": "^18.2.4",
    "@types/jest": "^29.5.12",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^20.14.9",
    "@vitejs/plugin-vue": "^5.0.5",
    "@vue/eslint-config-prettier": "^9.0.0",
    "@vue/eslint-config-typescript": "^13.0.0",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.5.1",
    "autoprefixer": "^10.4.19",
    "dotenv": "^16.4.5",
    "eslint": "^8.57.0",
    "eslint-plugin-vue": "^9.26.0",
    "jest": "^29.7.0",
    "jsdom": "^24.1.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.38",
    "prettier": "^3.3.2",
    "sass": "^1.77.6",
    "supabase": "^1.178.2",
    "supertest": "^7.0.0",
    "tailwindcss": "^3.4.4",
    "ts-jest": "^29.1.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.2.2",
    "vite": "^5.3.1",
    "vue-tsc": "^2.0.22"
  }
}
